
public class Soru2 {

	public static int[] tekrar(int[] dizi) {
		int[] dizi1= {1,2,2,2,3,4,5};
		int sayac=0;
		for (int i=0; i<dizi1.length; i++) {
			for(int j=0; j<dizi1.length; j++) {
				if( (!(i==j)) && (dizi1[i]==dizi1[j])) {
					sayac++;
				}
			}	
		}
		
		System.out.println( dizi1[i] + "sayısı" + sayac + "tekrar ediyor");
		return dizi;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] dizi= {1,2,2,2,3,4,5};
		int[] dizi1 =tekrar(dizi);
		
		
	}

}
