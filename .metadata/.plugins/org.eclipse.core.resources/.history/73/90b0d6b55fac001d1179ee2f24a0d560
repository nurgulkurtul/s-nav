
public class Soru2 {

	public static int[] tekrar(int[] dizi) {
		int[] dizi1= {1,2,2,3,4};
		
		int sayac=0;
		for (int i=0; i<dizi1.length; i++) {
			for(int j=0; j<dizi1.length; j++) {
				if( ()!(i=j)) && dizi1[i]==dizi1[j]) {
					sayac++;
				}
				
			}
				
		}
		return dizi;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] dizi1= {1,2,2,3,2,4};
		int[] dizi =tekrar(dizi1);
		
		
	}

}
