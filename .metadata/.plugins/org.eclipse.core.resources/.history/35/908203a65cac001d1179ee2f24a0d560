
public class Soru2 {

	public static int tekrar(int[] dizi) {
//		int[] atama=new int[dizi.length];
		int sayac=0;
		for (int i=0; i<dizi.length; i++) {
			for(int j=0; j<dizi.length; j++) {
				if((! (i==j)) && dizi[i]==dizi[j]) {
					System.out.println(sayac);
					sayac++;
				}
				
			}
				
		}
		return sayac;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] dizi={1,2,5,5,5,3,4};
		int[] dizi2=tekrar(dizi);
		
		System.out.println(tekrar(dizi2));
	}

}
